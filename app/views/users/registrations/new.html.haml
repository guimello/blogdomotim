%h2 Sign up
= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
  =# devise_error_messages!
  = f.error_messages
  %p
    = f.label :email
    %br/
    = f.text_field :email
  %p
    = f.label :password
    %br/
    = f.password_field :password
  %p
    = f.label :password_confirmation
    %br/
    = f.password_field :password_confirmation
  %p
    = f.label :name
    %br/
    = f.text_field :name
  %p
    = f.label :birthday
    %br/
    = f.text_field :birthday, :size => 10, :class => :datepicker, :value => f.object.birthday.present? ? l(f.object.birthday) : f.object.birthday_before_type_cast


  %p= f.submit "Sign up"
= render :partial => "devise/shared/links"
